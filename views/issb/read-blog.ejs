<script>
  const times = document.querySelectorAll('.time');

// Iterate over each element with the class name "time"
times.forEach(time => {
    // Get the text content of the element (createdAt from MongoDB)
    const createdAt = new Date(time.textContent.trim());

    // Months array for mapping month indexes
    const months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];

    // Format the date as "Month Day, Year"
    const formattedDate = `${months[createdAt.getMonth()]} ${createdAt.getDate()}, ${createdAt.getFullYear()}`;

    // Update the text content of the element with the formatted date
    time.textContent = formattedDate;
});
</script>